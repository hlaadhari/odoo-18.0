<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Données de base STEG -->
        
        <!-- Séquences pour les codes STEG -->
        <record id="seq_steg_spare_part" model="ir.sequence">
            <field name="name">Pièces de Rechange STEG</field>
            <field name="code">steg.spare.part</field>
            <field name="prefix">STEG-</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_steg_equipment" model="ir.sequence">
            <field name="name">Équipements STEG</field>
            <field name="code">steg.equipment</field>
            <field name="prefix">EQ-</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_steg_stock_move" model="ir.sequence">
            <field name="name">Mouvements Stock STEG</field>
            <field name="code">steg.stock.move</field>
            <field name="prefix">MV-</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <record id="seq_steg_inventory" model="ir.sequence">
            <field name="name">Inventaires STEG</field>
            <field name="code">steg.inventory</field>
            <field name="prefix">INV-</field>
            <field name="suffix">-%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

    </data>
</odoo>